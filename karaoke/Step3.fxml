<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="700.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="karaoke.Step3">
   <bottom>
      <Button fx:id="btnNext" mnemonicParsing="false" onAction="#next" prefWidth="800.0" text="다음" BorderPane.alignment="CENTER" />
   </bottom>
   <top>
      <Button mnemonicParsing="false" onAction="#prev" prefWidth="800.0" text="이전" BorderPane.alignment="CENTER" />
   </top>
   <center>
      <ScrollPane hbarPolicy="NEVER" prefHeight="200.0" prefWidth="200.0" vbarPolicy="ALWAYS" BorderPane.alignment="CENTER">
         <content>
            <VBox fx:id="content" prefWidth="800.0" spacing="30.0" />
         </content>
      </ScrollPane>
   </center>
</BorderPane>
